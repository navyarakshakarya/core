{
  "name": "@nrksolusi/core",
  "version": "1.3.5",
  "description": "Navya Raksha Karya Core Library",
  "main": "index.js",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "npm run build",
    "publish": "npm run build && npx semantic-release",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navyarakshakarya/core.git"
  },
  "keywords": [
    "core",
    "library"
  ],
  "author": "navya raksha karya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/navyarakshakarya/core/issues"
  },
  "homepage": "https://github.com/navyarakshakarya/core#readme",
  "dependencies": {
    "dotenv": "^16.4.5",
    "mongoose": "^8.7.1",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "jsonwebtoken": "^9.0.2",
    "semantic-release": "^24.1.2",
    "ts-node": "^10.9.2"
  },
  "files": [
    "dist/*",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      },
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": "beta"
      },
      {
        "name": "rc",
        "channel": "rc",
        "prerelease": "rc"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
